{
    "name": "mikejw/empathy",
    "type": "library",
    "description": "Empathy PHP Framework",
    "keywords": [
        "framework",
        "mvc"
    ],
    "homepage": "https://empathy.sh",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J. Whiting",
            "email": "mail@mikejw.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mikejw/alice"
        }
    ],
    "require": {
        "php": ">=8.4.0",
        "wp-cli/php-cli-tools": "dev-master",
        "mustangostang/spyc": "0.6.3",
        "mikejw/elibs": "^2",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^2.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.7.2",
        "mockery/mockery": "^1.3.0",
        "nelmio/alice": "dev-2.x-dev"
    },
    "autoload": {
        "psr-0": {
            "Empathy": "src/",
            "Psr": "src/"
        }
    },
    "bin": [ "bin/empathy" ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
