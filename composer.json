{
    "name": "mikejw/empathy",
    "type": "library",
    "description": "Empathy PHP Framework",
    "keywords": [
        "framework",
        "mvc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Whiting",
            "email": "mikejw3@gmail.comk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "wp-cli/php-cli-tools": "dev-master",
        "mustangostang/spyc": "0.6.3",
        "mikejw/elibs": "dev-develop",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0", 
        "squizlabs/php_codesniffer": "^3.1.0",
        "mayflower/php-codebrowser": "1.1.4",
        "mockery/mockery": "^1.3.0",
        "nelmio/alice": "2.2.2",
        "phpdocumentor/phpdocumentor": "^2.9.1",
        "pheromone/phpcs-security-audit": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Empathy": "src/",
