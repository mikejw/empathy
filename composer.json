{
    "name": "mikejw/empathy",
    "type": "library",
    "description": "Empathy PHP Framework",
    "keywords": [
        "framework",
        "mvc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Whiting",
            "email": "mikejw3@gmail.comk"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "wp-cli/php-cli-tools": "dev-master",
        "mustangostang/spyc": "0.6.3",
        "mikejw/elibs": "dev-develop",
        "php-di/php-di": "^6.0",
        "monolog/monolog": "^2.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.7.2",
        "mockery/mockery": "^1.3.0",
        "nelmio/alice": "^2"
    },
    "autoload": {
        "psr-0": {
            "Empathy": "src/",
            "Psr": "src/"
        }
    },
    "bin": [ "bin/empathy" ],
    "minimum-stability": "dev",
    "prefer-stable": true
